<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>


<% @users.each do |user| %>
	<%= user.first_name %>
<% end %>

<%= form_tag("/search", method: "get") do %>
  <%= label_tag(:q, "Search for a category:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<p>Business Name: <%= @res_name %></p>
<p>Distance from you, in meters: <%= @res_dist %></p>
<p>Business Address: <%= @res_addr %></p>

<div><h1 id='map'>Here is our Map, made by appending Handlebars</h1></div>


